{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "AiEDU|API文档",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "appearance": {
    "default": "light",
    "strict": false
  },
  "favicon": "/avatar.png",
  "navigation": {
    "tabs": [
      {
        "tab": "指南",
        "groups": [
          {
            "group": "文档撰写",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "图像生成",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "代码编写",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "教育教学",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API接口",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "博客",
          "href": "https://googhub.nyc.mn/",
          "icon": "book-open-cover"
        },
        {
          "anchor": "导航",
          "href": "https://518855.xyz/",
          "icon": "slack"
        },
        {
          "anchor": "About",
          "href": "https://googhub.nyc.mn/",
          "icon": "user"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:aiedu985@gmail.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "联系我",
      "href": "https://googhub.nyc.mn/"
    }
  },
  "contextual": {
